<AnchorPane focusTraversable="true" onKeyPressed="#handleKeyPressed" prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Plansza">
   <children>
      <VBox fx:id="plansza" layoutX="99.0" layoutY="74.0" onKeyPressed="#handleKeyPressed" prefHeight="253.0" prefWidth="454.0" AnchorPane.bottomAnchor="100.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
      <HBox fx:id="exitBtn" onKeyPressed="#handleKeyPressed" prefHeight="100.0" prefWidth="604.0" spacing="20.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="500.0">
         <children>
            <Button fx:id="safe" onKeyPressed="#handleKeyPressed" maxHeight="50.0" maxWidth="100.0" mnemonicParsing="false" onAction="#safeGame" prefHeight="50.0" prefWidth="100.0" text="Zapisz grę" />
            <Button fx:id="closeBtn" onKeyPressed="#handleKeyPressed" maxHeight="50.0" maxWidth="100.0" mnemonicParsing="false" onAction="#closeGame" prefHeight="50.0" prefWidth="100.0" text="Wyjdź" />
            <Button fx:id="click" onKeyPressed="#handleKeyPressed" maxHeight="50.0" maxWidth="100.0" mnemonicParsing="false" onAction="#click" prefHeight="50.0" prefWidth="100.0" text="Button" />
